
********************************************
class Solution {
public:
    int lengthOfLastWord(string s) {
        int count = 0;
        int len = s.length() - 1;
        while(len>=0 and s[len]==' '){
            len--;
        }
        while(len >= 0 and s[len]!=' '){
            count++;
            len--;
        }
        return count;
    }
};
